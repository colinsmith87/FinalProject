<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.smithsiciliano.models.Employee" table="EMPLOYEE">
        <id name="employeeId" type="int">
            <generator class="increment" />
        </id>
        <property name="fName" type="string">
        	<column name="FNAME"/>
        </property>
        <property name="lName" type="string">
        	<column name="LNAME"/>
        </property>
        <property name="salary" type="int">
        	<column name="SALARY"/>
        </property>
        <property name="phone" type="long">
        	<column name="PHONE"/>
        </property>
        <property name="street" type="string">
        	<column name="STREET"/>
        </property>
        <property name="city" type="string">
        	<column name="CITY"/>
        </property>
        <property name="addState" type="string">
        	<column name="ADDSTATE"/>
        </property>
        <property name="zip" type="int">
        	<column name="ZIP"/>
        </property>
        <property name="dateOfBirth" type="string">
        	<column name="DATEOFBIRTH"/>
        </property>
        <property name="storeLoc" type="string">
        	<column name="STORELOC"/>
        </property>
    </class>
</hibernate-mapping>