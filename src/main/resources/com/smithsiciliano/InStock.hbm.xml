<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.smithsiciliano.models.InStock" table="IN_STOCK">
		<composite-id>
			<key-property name="foodItem" type="string">
				<column name="FOODITEM" length="255" not-null="true" />
			</key-property>
			<key-property name="storeLoc" type="string">
				<column name="STORELOC" length="255" not-null="true"/>
			</key-property>
		</composite-id>
		<property name="quantity" type="int">
			<column name="QUANTITY"/>
		</property>
	</class>
</hibernate-mapping>