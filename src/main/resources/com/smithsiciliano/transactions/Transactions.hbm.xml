<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.smithsiciliano.models.Transactions" table="TRANSACTIONS">
        <id name="transactionId" type="int">
            <generator class="increment" />
        </id>
        <property name="price" type="double">
        	<column name="PRICE"/>
        </property>
        <property name="foodItem" type="string">
        	<column name="FOODITEM"/>
        </property>
        <property name="storeLoc" type="string">
        	<column name="STORELOC"/>
        </property>
    </class>
</hibernate-mapping>